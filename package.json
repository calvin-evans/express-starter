{
  "name": "express-starter",
  "author": "CEUK",
  "license": "MIT",
  "version": "0.3.0",
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "tslint -c tslint.json -p tsconfig.json \"./src/**/*.ts\"",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc",
    "dev:start": "ts-node src",
    "dev": "DEBUG=server* nodemon --watch src -e ts --exec npm run dev:start",
    "start": "DEBUG=server:*:info node build",
    "swagger": "swaggerGen -c ./swagger.config.yml -p ./tsconfig.json",
    "e2e": "riteway -r @babel/polyfill ' __tests__/**/*' -r './babel-register.js' | tap-nirvana",
    "unit": "riteway -r @babel/polyfill 'src/**/*.spec*' -r './babel-register.js' | tap-nirvana",
    "unit:watch": "chokidar \"src/**/*.ts\" -c \"npm run unit\"",
    "test": "npm run unit && npm run e2e",
    "coverage": "nyc npm run test"
  },
  "dependencies": {
    "@versita/fp-lib": "^1.0.15",
    "acl-sequelize": "^0.3.5",
    "babel-plugin-import": "^1.13.0",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "got": "^10.6.0",
    "helmet": "^4.2.0",
    "http-errors": "^1.7.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.0",
    "node-env-file": "^0.1.8",
    "node-polyglot": "^2.4.0",
    "nyc": "^15.1.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.4.1",
    "pg-hstore": "^2.3.3",
    "ramda": "^0.27.1",
    "reflect-metadata": "^0.1.13",
    "require-glob": "^3.2.0",
    "sequelize": "^5.22.3",
    "sequelize-typescript": "^1.1.0",
    "swagger-ui-dist": "^3.35.2",
    "swagger-ui-express": "^4.1.5",
    "typescript-rest": "^3.0.2",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@alt3/sequelize-to-json-schemas": "^0.3.52",
