  "version": "0.2.0",
  "excludeModules": [
    "Bcrypt.Base"
  ],
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug",
      "program": "${workspaceFolder}/src/index.js",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/babel-node",
      "runtimeArgs": [
        "--nolazy"
      ],
      "runtimeVersion": "10.15.2",
      "env": {
        "DEBUG": "centrum*",
        "BABEL_ENV": "debug"
      }
    }
  ]
}
